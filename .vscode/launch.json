   {
     "version": "0.2.0",
     "configurations": [
       {
         "name": "Extension: Run Code Improver",
         "type": "extensionHost",
         "request": "launch",
         "runtimeExecutable": "${execPath}",
         "args": [
           "--extensionDevelopmentPath=${workspaceFolder}/frontend"
         ],
         "outFiles": [
           "${workspaceFolder}/frontend/out/**/*.js"
         ],
         "preLaunchTask": "frontend: compile"
       }
     ]
   }